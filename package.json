  "scripts": {
    "deps-server": "npm ci --prefix backend",
    "deps-client": "npm ci --prefix frontend",
    "deps": "npm ci && npm-run-all -p deps-server deps-client",
    "server-dev": "cd backend && npm run dev",
    "server": "npm start --prefix backend",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server-dev\" \"npm run client\""
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "npm-run-all": "^4.1.5"
  },
